<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fox.telehealthmeditek" >

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-feature android:name="android.hardware.camera" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".ActivationActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".VerifyCodeActivity"
            android:label="@string/title_activity_verify_code" >
        </activity>
        <activity
            android:name=".HomeActivity"
            android:label="@string/title_activity_home" >
        </activity>
        <activity
            android:name=".CallActivity"
            android:label="@string/title_activity_call" >
        </activity>
        <activity
            android:name=".VoiceActivity"
            android:label="@string/title_activity_voice" >
        </activity>
        <activity
            android:name=".DisplayGalleyPhotoActivity"
            android:label="@string/title_activity_display_galley_photo" >
        </activity>
    </application>

</manifest>
