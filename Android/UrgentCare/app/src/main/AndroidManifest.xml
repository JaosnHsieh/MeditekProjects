<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.redimed.urgentcare" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity android:name=".HomeActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MakeAppointmentActivity"
            android:label="@string/title_activity_make_appointment" >
        </activity>
        <activity
            android:name=".FAQ"
            android:label="@string/title_activity_faq" >
        </activity>
        <activity
            android:name=".FAQActivity"
            android:label="@string/title_activity_faq" >
        </activity>
        <activity
            android:name=".SportInjuryActivity"
            android:label="@string/title_activity_sport_injury" >
        </activity>
        <activity
            android:name=".WorkInjuryActivity"
            android:label="@string/title_activity_work_injury" >
        </activity>
    </application>

</manifest>
