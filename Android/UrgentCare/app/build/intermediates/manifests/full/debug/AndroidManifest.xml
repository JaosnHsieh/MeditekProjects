<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.redimed.urgentcare"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity android:name="com.redimed.urgentcare.HomeActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.redimed.urgentcare.MakeAppointmentActivity"
            android:label="@string/title_activity_make_appointment" >
        </activity>
        <activity
            android:name="com.redimed.urgentcare.FAQActivity"
            android:label="@string/title_activity_faq" >
        </activity>
        <activity
            android:name="com.redimed.urgentcare.SportInjuryActivity"
            android:label="@string/title_activity_sport_injury" >
        </activity>
        <activity
            android:name="com.redimed.urgentcare.WorkInjuryActivity"
            android:label="@string/title_activity_work_injury" >
        </activity>
    </application>

</manifest>