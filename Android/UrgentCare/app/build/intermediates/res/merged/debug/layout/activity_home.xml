<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:fbutton="http://schemas.android.com/apk/res-auto"
    tools:context="com.redimed.urgentcare.HomeActivity">

    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:scaleType="fitXY"
        android:id="@+id/imgBackgroundHome"
        android:background="#f8f9fa"/>

    <ImageView
        android:layout_width="300dp"
        android:layout_height="59dp"
        android:id="@+id/imgLogoRedimed"
        android:layout_centerHorizontal="true" />

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:id="@+id/linearLayout1">
                <RelativeLayout
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    android:background="@drawable/img_call_us">
                    <com.andexert.library.RippleView
                        android:id="@+id/rippleViewPlasticInjury"
                        android:layout_width="90dp"
                        android:layout_height="90dp"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentBottom="true"
                        android:layout_marginBottom="10dp"
                        android:layout_marginRight="10dp"
                        rv_centered="true"
                        android:background="@android:color/transparent">
                    </com.andexert.library.RippleView>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    android:background="@drawable/img_sport_injury">
                    <com.andexert.library.RippleView
                        android:id="@+id/rippleViewSportInjury"
                        android:layout_width="90dp"
                        android:layout_height="90dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginBottom="10dp"
                        rv_centered="true"
                        android:layout_alignParentBottom="true"
                        android:background="@android:color/transparent">
                    </com.andexert.library.RippleView>
                </RelativeLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_below="@+id/linearLayout1">
                <RelativeLayout
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    android:background="@drawable/img_work_injury">
                    <com.andexert.library.RippleView
                        android:id="@+id/rippleViewWorkInjury"
                        android:layout_width="90dp"
                        android:layout_height="90dp"
                        rv_centered="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginTop="10dp"
                        android:layout_marginRight="10dp"
                        android:background="@android:color/transparent">
                    </com.andexert.library.RippleView>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    android:background="@drawable/img_faq">
                    <com.andexert.library.RippleView
                        android:id="@+id/rippleViewOrthopaedic"
                        android:layout_width="90dp"
                        android:layout_height="90dp"
                        rv_centered="true"
                        android:layout_marginLeft="10dp"
                        android:background="@android:color/transparent">
                    </com.andexert.library.RippleView>
                </RelativeLayout>
            </LinearLayout>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:background="@drawable/btn_urgentcare">
        <com.andexert.library.RippleView
            android:id="@+id/rippleViewCallUs"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_centerHorizontal="true"
            rv_centered="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:background="@android:color/transparent">
        </com.andexert.library.RippleView>
    </RelativeLayout>



    <LinearLayout
        android:layout_width="275dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:orientation="horizontal"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="40dp">
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <info.hoang8f.widget.FButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/btnUrgentCare"
                android:text="URGENT CARE"
                android:layout_alignParentLeft="true"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:textColor="#FFFFFF"
                fbutton:buttonColor="@color/fbutton_color_turquoise"
                fbutton:shadowColor="@color/fbutton_color_green_sea"
                fbutton:shadowEnabled="true"
                fbutton:shadowHeight="5dp"
                fbutton:cornerRadius="5dp"/>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <info.hoang8f.widget.FButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/btnFaq"
                android:layout_alignParentRight="true"
                android:layout_marginLeft="50dp"
                android:text="FAQs"
                android:paddingLeft="38dp"
                android:paddingRight="38dp"
                android:textColor="#FFFFFF"
                fbutton:buttonColor="@color/fbutton_color_wet_asphalt"
                fbutton:shadowColor="@color/fbutton_color_midnight_blue"
                fbutton:shadowEnabled="true"
                fbutton:shadowHeight="5dp"
                fbutton:cornerRadius="5dp"/>
        </RelativeLayout>
    </LinearLayout>
    <ImageView
        android:layout_width="300dp"
        android:layout_height="20dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="10dp"
        android:background="@drawable/lblslogan"
        android:layout_centerHorizontal="true"/>
</RelativeLayout>
